  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "test": "echo test",
    "lint:fix": "eslint . --fix",
    "start:app": "pnpm run dev:main --filter=./app",
    "ci:publish": "pnpm run build && pnpx changeset publish",
    "ci:version": "pnpx changeset version",
    "ci:snapshot": "pnpx changeset version --snapshot beta",
    "ci:prerelease": "pnpm run build && pnpx changeset publish --tag beta",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/**/*.{js,ts,tsx,vue,json}": [
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@aiou/eslint-config": "^0.3.1",
    "@changesets/cli": "^2.16.0",
    "cz-emoji": "^1.3.1",
    "eslint": "^7.32.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.1.1",
    "prettier": "^2.3.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-emoji"
    }
  }
